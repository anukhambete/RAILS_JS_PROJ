<div class="background-image-place">

<div class="places_new">

<h1>Edit Place Form</h1>

<%= form_for (@place) do |f| %>
  <!-- <%= f.collection_check_boxes :name, Place.all, :id, :name %></br></br> -->
<%= render partial: "form", locals: {place: @place, f: f} %>
<%= f.hidden_field :current_user_id, value: @current_user.id %>
<%= f.submit "Edit place" %>
<% end %>

<h3><%= link_to "<< Return to List of Places", places_path %></h3>
</div>
</div>
